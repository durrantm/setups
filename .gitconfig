[user]
	name = Michael Durrant
	email = m2@snap2web.com
[push]
	default = current
[alias]
  co = checkout
  ci = commit -v
	comit = commit -v
  set-up = !git branch --set-upstream-to=origin/`git symbolic-ref --short HEAD`
[branch]
	autosetuprebase = always
[core]
	excludesfile = ~/.gitignore_global
	editor = vim
[color]
  ui = true
